<main>

    <div class="container">
        <h1><%= title %></h1>
    </div>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Google Id</th>
                <th scope="col">Email</th>
                <th scope="col">Users Image</th>
                <th scope="col">Users Type</th>
                <th scope="col">Created</th>
                <th scope="col">Updated</th>
                
            </tr>
        </thead>

        <% users.forEach(user => { %>
            <tr>
                <td><%= user.name %></td>
                <td><%= user.googleId %></td>
                <td><%= user.email %></td>
                <td><img src="<%= user.avatar %>" referrerpolicy="no-referrer" height="35px"></img></td>
                <td><%= user.userType %></td>
                <td> Created <%= dayjs(user.createdAt).format('DD/MM/YYYY') %></td>
                <td> Updated <%= dayjs(user.updatedAt).fromNow(true) %> ago</td>
            </tr>        
                
            <td><a href="/user/edit?id=<%=user._id %>">Edit User</a></td>

            <%  
        });%>


    </table>

</main>